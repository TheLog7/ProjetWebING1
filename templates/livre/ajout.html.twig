{% extends 'base.html.twig' %}

{% block title %}Ajouter un livre{% endblock %}

{% block body %}
    <h1>Ajouter un livre</h1>

    {{ form_start(form) }}
        {{ form_row(form.titre) }}
        {{ form_row(form.nom_auteur) }}
        {{ form_row(form.prenom_auteur) }}
    {{ form_end(form) }}

    <a href="{{ path('app_bibliotheque') }}">Retour à la bibliothèque</a>
{% endblock %}